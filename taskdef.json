{
    "jobDefinitionName": "payfacto-auto-test",
    "type": "container",
    "parameters": {},
    "retryStrategy": {
        "attempts": 1
    },
    "containerProperties": {
        "image": "{{image}}",
        "jobRoleArn": "{{jobRoleArn}}",
        "vcpus": 16,
        "memory": 1024
        "environment": [
            {
                "name": "CDN_ENV",
                "value": "dev"
            },
            {
                "name": "DB_ENV",
                "value": "dev"
            }
        ]
    },
    "timeout": {
        "attemptDurationSeconds": 100
    }
}
